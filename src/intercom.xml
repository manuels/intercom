<?xml version="1.0" encoding="UTF-8" ?>

<node name="/org/manuel/Intercom">
<interface name='org.manuel.Intercom'>
	<method name="connect">
		<annotation name="org.gtk.GDBus.C.UnixFD" value="true"/>

		<arg type="i" name="socket_type" direction="in" />
		<arg type="ay" name="remote_public_key" direction="in" />
		<arg type="u" name="port" direction="in" />
		<arg type="u" name="timeout" direction="in" />

		<arg type="h" name="fd" direction="out" />
	</method>

	<method name="add_node">
		<arg type="as" name="node_name" direction="in" />
		<arg type="ay" name="remote_public_key" direction="in" />
	</method>

	<method name="find_node">
		<arg type="as" name="keywords" direction="in" />
		<arg type="aay" name="nodes" direction="out" />
	</method>
</interface>
</node>
